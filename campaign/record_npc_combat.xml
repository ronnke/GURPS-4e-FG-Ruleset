<root>
	
	<windowclass name="npcsheet_combat">
		<script file="campaign/scripts/npc_combat.lua" />
		<sheetdata>
			<anchor_column name="contentanchor">
				<bounds>1,0,-1,0</bounds>
			</anchor_column>

			<number_attribute name="dodge" source="combat.dodge">
				<rollable_dodge />
				<anchored to="contentanchor" >
					<top offset="10" />
					<left anchor="center" offset="-100"/>
				</anchored>
				<tabtarget next="parry" />
        <default>0</default>
      </number_attribute>
			<label_attribute>
				<anchored to="dodge" />
				<static textres="char_label_dodge" />
			</label_attribute>

			<number_attribute name="parry" source="combat.parry">
				<rollable_parry />
				<anchored to="contentanchor" >
					<top offset="10" />
					<left anchor="center" offset="-45"/>
				</anchored>
				<tabtarget prev="dodge" next="block" />
        <default>0</default>
      </number_attribute>
			<label_attribute>
				<anchored to="parry" />
				<static textres="char_label_parry" />
			</label_attribute>

			<number_attribute name="block" source="combat.block">
				<rollable_block />
				<anchored to="contentanchor" >
					<top offset="10" />
					<left anchor="center" offset="10"/>
				</anchored>
				<tabtarget prev="parry" next="dr" />
        <default>0</default>
      </number_attribute>
			<label_attribute>
				<anchored to="block" />
				<static textres="char_label_block" />
			</label_attribute>

			<string_attribute name="dr" source="combat.dr">
				<anchored to="contentanchor" >
					<top offset="10" />
					<left anchor="center" offset="65"/>
				</anchored>
				<tabtarget prev="block" />
			</string_attribute>
			<label_attribute>
				<anchored to="dr" />
				<static textres="char_label_dr" />
			</label_attribute>

			<anchor_column name="columnanchor">
				<anchored to="dr" />
			</anchor_column>

			<header_column name="header_meleecombat">
				<anchored>
					<top parent="columnanchor" anchor="bottom" relation="relative" offset="50" />
				</anchored>
				<static textres="npc_label_melee" />
				<center />
				<color>#808080</color>
			</header_column>
			<button_iedit name="meleecombat_iedit">
				<anchored to="header_meleecombat" position="righthigh" offset="-20,0" />
				<target>meleecombat</target>
			</button_iedit>
			<button_iadd name="meleecombat_iadd">
