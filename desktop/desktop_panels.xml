<root>

  <panel name="chat">
    <anchored width="400">
      <left offset="5" />
      <top offset="120" />
      <bottom offset="-140" />
    </anchored>
    <class>chat</class>
    <dynamic />
    <locked />
  </panel>

  <panel name="modifierstack" modes="host,client">
		<class>modifierstack</class>
		<bounds>15,-100,64,57</bounds>
		<dynamic />
		<locked />
	</panel>

	<panel name="dicetower" modes="host,client">
		<anchored height="90" width="90">
			<bottom offset="-35" />
			<left parent="modifierstack" anchor="right" offset="50" />
		</anchored>
		<class>dicetower</class>
		<dynamic />
		<locked />
	</panel>
  
</root>