<root>
	<windowclass name="combattracker_host" merge="join">
		<placement>
			<size width="650" />
		</placement>
		<sizelimits>
			<minimum width="650" />
		</sizelimits>
		<sheetdata>
      <label_ct_header_init name="label_init" merge="delete" />

      <spacer_ct_host_header_button_skip name="spacer_buttons_header_add" insertbefore="label_name" />

      <label_ct_header_fatigue name="label_fatigue" insertbefore="label_name" />
      <label_ct_header_injury name="label_injury" insertbefore="label_name" />
      <label_ct_header_condition name="label_condition" insertbefore="label_name" />
      <label_ct_header_fps name="label_fps" insertbefore="label_name" />
      <label_ct_header_hps name="label_hps" insertbefore="label_name" />
      <label_ct_header_speed name="label_speed" insertbefore="label_name" />
    </sheetdata>
	</windowclass>
</root>
